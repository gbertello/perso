doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    title
    link(rel="stylesheet", href="css/style.css", type="text/css")
    link(rel="stylesheet", href="css/print.css", type="text/css", media="print")
  body
    div#page
      div#top
        h1#name
        ul#contact
          li#address
          li Tel:&nbsp;
            span#telephone
          li#email
          li#maritalStatus
        img#photo(alt="Photo")
      div#content
        div.section
          em#motivation
        div.section
          h2.section-title Employment History
          hr
          div#employmentItems
            include subsection.pug
          a(href="#")#expandHiddenEmploymentItems [+]
          div.hidden#hiddenEmploymentItems
            include subsection.pug
        div.section
          h2.section-title Education
          hr
          div#educationItems
            include subsection.pug
          a(href="#")#expandHiddenEducationItems [+]
          div.hidden#hiddenEducationItems
            include subsection.pug
        div.section
          h2.section-title Programming Skills
          hr
          div
            ul#programmingSkills
        div.section
          h2.section-title Personal Interests
          hr
          div
            ul#personalInterests
        div#footer Created with HTML/CSS/JS. Last updated on 01/06/2019.
    script(src="js/jquery.min.js", type="text/javascript")
    script(src="js/application.js", type="text/javascript")
    script(src="js/data.js", type="text/javascript")
    script(src="js/display.js", type="text/javascript")



